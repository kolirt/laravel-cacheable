{
    "name": "kolirt/laravel-cacheable",
    "description": "Easily cache and control class methods without having to remember cache key names",
    "type": "library",
    "keywords": [
        "laravel",
        "cache",
        "laravel-cache",
        "cacheable",
        "cacheable-methods",
        "cacheable-method"
    ],
    "homepage": "https://github.com/kolirt/laravel-cacheable",
    "license": "MIT",
    "version": "1.0.1",
    "authors": [
        {
            "name": "kolirt"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": ">=10.0"
    },
    "autoload": {
        "psr-4": {
            "Kolirt\\Cacheable\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kolirt\\Cacheable\\ServiceProvider"
            ]
        }
    }
}